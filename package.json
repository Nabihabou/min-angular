{
  "name": "min-angular",
  "description": "Minimal AngularJS setup",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmitriz/min-angular"
  },
  "author": {
    "name": "Dmitri Zaitsev",
    "email": "dmitri14@gmail.com",
    "url": "https://github.com/dmitriz"
  },
  "scripts": {
    "start": "open demo/index.html",
    "pub": "npm version patch && npm publish && git push",
    "sync": "git ac && git pull --rebase && git push",
    "postinstall": "node index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "demo"
  ],
  "keywords": [
    "angular",
    "bootstrap"
  ],
  "dependencies": {
    "angular": "^1.5.0",
    "gently-copy": "^2.1.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^1.1.5",
    "semantic-release": "^6.3.2"
  },
  "standard": {
    "globals": [
      "describe",
      "expect",
      "it",
      "beforeEach",
      "angular",
      "document"
    ]
  },
  "bugs": {
    "url": "https://github.com/dmitriz/min-angular/issues"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
